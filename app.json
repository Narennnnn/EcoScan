{
  "expo": {
    "name": "ecoscanapp",
    "slug": "ecoscanapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.narendrrrr.ecoscanapp",
      "icon": "./assets/images/ecoscanapp.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ecoscanapp.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.narendrrrr.ecoscanapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow EcoScan to access your photos to analyze clothing items.",
          "cameraPermission": "Allow EcoScan to access your camera to capture clothing items."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "101008fd-c3b3-4a42-b7ac-0f424c17a443"
      }
    },
    "owner": "narendrrrr"
  }
}
